<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table class='table table-bordered'>
  <thead>
    <tr>
      <th>Name</th>
      <th>email</th>
      <th>Age</th>
      <th>Gender</th>
      <th>Age</th>
      <th>Number</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user["first_name"] + " " + user["last_name"]%></td>
        <td><%= user["email"] %></td>
        <td><%= user["age"] %></td>
        <td><%= user["email"] %></td>
        <td><%= user["gender"] %></td>
        <td><%= user["mobile_number"] %></td>
      </tr>
    <% end %>
  </tbody>
</table>